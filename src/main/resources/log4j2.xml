<?xml version="1.0" encoding="UTF-8"?>
<Configuration packages="org.logstash.log" status="INFO">
  <Appenders>
    <Console name="Console" target="SYSTEM_OUT">
      <JSONObjectLayout/>
    </Console>
  </Appenders>
  <Loggers>
    <Root level="info">
      <AppenderRef ref="Console"/>
    </Root>
  </Loggers>
</Configuration>
